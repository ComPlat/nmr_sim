{
  "extends": "airbnb",
  "rules": {
    "arrow-body-style": 0,
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "react/jsx-no-bind": 0,
    "react/jsx-uses-react": 1,
    "react/prefer-stateless-function": 0,
    "react/jsx-closing-bracket-location": [0, {"location": "after-props"}],
    "react/jsx-indent-props": 1,
    "no-use-before-define": 1
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
